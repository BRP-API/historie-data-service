{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Brp": "Debug",
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "Ocelot": "Error"
      }
    }
  },
  "Ecs": {
    "Path": "../../test-data/logs/brp-autorisatie.json",
    "FileSizeLimitBytes": "10240"
  },
  "OAuth": {
    "Authority": "http://identityserver:6000"
  },
  "FeatureManagement": {
    "gebruikMeestRecenteAutorisatie": false
  }
}
